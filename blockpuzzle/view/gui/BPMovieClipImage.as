import blockpuzzle.controller.game.*;import blockpuzzle.view.gui.*;class blockpuzzle.view.gui.BPMovieClipImage extends BPGuiElement {        var clipName:String;        function BPMovieClipImage(controller:BPController, clipName:String) {        super(controller, true);                this.clipName = clipName;        //        var data = flash.display.BitmapData.loadBitmap(clipName);       //trace( movieClip.attachMovie(clipName, clipName, 0))    }    /***************************    *                          *    * Loading & Saving Methods *    *                          *    ***************************/        function toXml(bank:String) {        return "<image source='" + clipName + "' bank='" + bank + "' left='" + left + "' top='" + top + "' scale='" + fraction + "' />\n";    }    static function loadFromXml(controller:BPController, xml:XML) {        return new BPMovieClipImage(controller, xml.attributes.source);    }}